{
  "manifest_version": 3,
  "name": "Mail IITB",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "identity",
    "sidePanel",
    "notifications"
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "action": {
    "default_title": "open IITB Mail Assistant"
  },
  "host_permissions": [
    "*://webmail.iitb.ac.in/*",
    "*://*.iitb.ac.in/*",
    "https://www.googleapis.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "key": "",
  "oauth2": {
    "client_id": "451389975470-e42fi26fo0gbde0d9ppafei19ctk63nb.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://webmail.iitb.ac.in/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
